{
  "name": "less-to-pallette",
  "version": "1.0.0",
  "description": "webpage to make working with less file colour variables more pleasant",
  "main": "index.js",
  "repository": "https://github.com/ThatCrazyIrishGuy/less-to-palette",
  "scripts": {
    "start": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/webpack-dev-server --colors --config webpack.config.js",
    "test": "jest --coverage",
    "test-ci": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "build": "node_modules/.bin/cross-env NODE_ENV=production node_modules/.bin/webpack -p --config ./webpack.production.config.js",
    "lint": "node_modules/.bin/eslint -c .eslintrc src tests --color",
    "lint-fix": "node_modules/.bin/eslint -c .eslintrc src tests  --color --fix"
  },
  "author": "Darren Britton",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-runtime": "^6.23.0",
    "caniuse-db": "^1.0.30000631",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "css-modules-require-hook": "^4.0.5",
    "enzyme": "^3.3.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-babel": "~4.1.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.3.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.0",
    "postcss-mixins": "^6.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.2",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.2.0",
    "coveralls": "3.0.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-preset-stage-1": "^6.1.18",
    "flexboxgrid": "^6.3.1",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^6.0.18",
    "postcss-cssnext": "^3.1.0",
    "preact": "^8.2.7",
    "preact-compat": "^3.13.1",
    "promise-file-reader": "^1.0.0",
    "query-string": "^5.1.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-css-modules": "^4.1.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-flexbox-grid": "^2.0.0",
    "react-flip-move": "^3.0.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-sparklines": "^1.6.0",
    "react-style-proptype": "^3.2.0",
    "react-toolbox": "^2.0.0-beta.6",
    "redux": "^3.6.0",
    "redux-promise": "^0.5.3",
    "tinycolor2": "^1.4.1"
  }
}
